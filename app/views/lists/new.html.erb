<div class="container">
  <div class="form">
    <%= simple_form_for(@list) do |f| %>
      <%= f.input :name %>
      <%= f.input :photo, as: :file %>
      <div class="btn">
        <%= f.button :submit  %>
      </div>
    <% end %>
  </div>
  <p>
  <%= link_to "Main list page", lists_path %>
  </p>
</div>
